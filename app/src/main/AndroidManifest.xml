<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.Kak_voobshe_eto_delat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:allowClearUserData="true"
        android:fullBackupContent="@xml/backup_rules"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Medic"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.CreateProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.CreatePasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.GetCodeEmailActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.AuthActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.OnBoardActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Kak_voobshe_eto_delat.LauncherActivity"
            android:exported="true">

            <!-- Запускаемое активити -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>